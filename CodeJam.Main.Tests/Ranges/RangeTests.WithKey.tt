<#@ template debug="false" hostspecific="true" language="C#" #>
<#@ include file="$(SolutionDir)Build\T4\Autogenerated.ttinclude" #>
<#@ include file="$(SolutionDir)Build\T4\Transform.ttinclude" #>
<#
		Transform(
			"RangeTests.NoKey.cs",
			new ReplaceInfo // Rename TestRange => TestRange
			{
				Pattern = "TestRange",
				Replacement = "TestKeyedRange",
				ThrowOnFail = true
			},
			new ReplaceInfo // Add RangeKey parameter to Create() calls
			{
				Pattern = @"(Range.Create.*?\([^,\)]+?,[^,\)]+?)\)",
				Replacement = @"$1, RangeKey)",
				ThrowOnFail = true
			});
#>
